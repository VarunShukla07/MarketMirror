{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AdfMirror"
		},
		"AzureDatabricksLinkedService_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksLinkedService'"
		},
		"ls_adls_raw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_raw'"
		},
		"AzureDatabricksLinkedService_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1017-105221-tp0btd0d"
		},
		"ls_adls_raw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stgmirror.dfs.core.windows.net/"
		},
		"ls_rest_alphavantage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.alphavantage.co/query"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AdfMirror_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksLinkedService_accessToken": {
						"value": "[parameters('AzureDatabricksLinkedService_accessToken')]"
					},
					"ls_adls_raw_accountKey": {
						"value": "[parameters('ls_adls_raw_accountKey')]"
					},
					"AzureDatabricksLinkedService_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksLinkedService_properties_typeProperties_existingClusterId')]"
					},
					"ls_adls_raw_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_raw_properties_typeProperties_url')]"
					},
					"ls_rest_alphavantage_properties_typeProperties_url": {
						"value": "[parameters('ls_rest_alphavantage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}